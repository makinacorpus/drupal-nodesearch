{
    "name": "makinacorpus/drupal-nodesearch",
    "type": "drupal-module",
    "description": "Node selector and search endpoint",
    "homepage": "http://github.com/makinacorpus/drupal-nodesearch",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name": "Makina Corpus",
            "homepage": "http://makina-corpus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\Drupal\\NodeSearch\\" : "src/"
        }
    },
    "requires": {
        "makinacorpus/calista-core": "*",
        "makinacorpus/drupal-sf-dic": "^3 || ^4"
