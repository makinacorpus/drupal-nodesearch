{
    "name": "makinacorpus/drupal-nodesearch",
    "type": "drupal-module",
    "description": "Node selector and search endpoint",
    "homepage": "http://github.com/makinacorpus/drupal-nodesearch",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name": "Makina Corpus",
            "homepage": "http://makina-corpus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\Drupal\\NodeSearch\\" : "src/"
        }
    },
    "require": {
        "drupal/core": "^7",
        "makinacorpus/calista-core": "^1",
        "makinacorpus/drupal-sf-dic": "^4"
    },
    "suggest": {
        "makinacorpus/drupal-ulink": "Simple entity or external link field, this modules brings a widget for it",
        "makinacorpus/drupal-unoderef": "Simple node reference field, this modules brings a widget for it"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
